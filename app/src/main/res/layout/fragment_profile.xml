<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/prof"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:background="#D5E6E6"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="@+id/constraintL"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:layout_width="100dp"
            android:id="@+id/showing"
            android:layout_height="80dp"
            android:src="@drawable/ic_baseline_home"></ImageView>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/constraintL"
        android:layout_width="0dp"
        android:layout_height="406dp"

        android:layout_marginStart="8dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout">

        <androidx.cardview.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="400dp"

            app:cardCornerRadius="5dp">


            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/ImageMan"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            tools:srcCompat="@drawable/ic_baseline_home"></ImageView>


                        <TextView
                            android:id="@+id/tvName"
                            android:layout_width="wrap_content"
                            android:layout_height="30dp"
                            android:layout_gravity="start"
                            android:layout_marginLeft="5dp"
                            android:gravity="center"
                            android:text="Name"
                            android:textSize="20dp" />




                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/ImgEmail"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="2dp"
                            tools:srcCompat="@drawable/ic_baseline_home"></ImageView>

                        <TextView
                            android:id="@+id/tvEmail"
                            android:layout_width="wrap_content"
                            android:layout_height="30dp"
                            android:layout_marginLeft="5dp"
                            android:gravity="center"
                            android:text="Email  "
                            android:textSize="20dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/ImagePhone"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            tools:srcCompat="@drawable/ic_baseline_home"></ImageView>


                        <TextView
                            android:id="@+id/tvMobileNo"
                            android:layout_width="wrap_content"
                            android:layout_height="30dp"
                            android:layout_marginLeft="5dp"
                            android:gravity="center"
                            android:text="Mobile No"
                            android:textSize="20dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/ImgCompany"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            tools:srcCompat="@drawable/ic_baseline_home"></ImageView>


                        <TextView
                            android:id="@+id/tvCompany"
                            android:layout_width="wrap_content"
                            android:layout_height="30dp"
                            android:layout_marginLeft="5dp"
                            android:gravity="center"
                            android:text="Company"
                            android:textSize="20dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:visibility="gone"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/ImgStore"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            tools:srcCompat="@drawable/ic_baseline_home"></ImageView>

                        <TextView
                            android:id="@+id/tvstore"
                            android:layout_width="wrap_content"
                            android:layout_height="30dp"
                            android:layout_marginLeft="5dp"
                            android:gravity="center"
                            android:text="Store"
                            android:textSize="20dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/ImgDoj"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            tools:srcCompat="@drawable/ic_baseline_home"></ImageView>

                        <TextView
                            android:id="@+id/tvDOJ"
                            android:layout_width="wrap_content"
                            android:layout_height="30dp"
                            android:layout_marginLeft="5dp"
                            android:gravity="center"
                            android:text="DOJ"
                            android:textSize="20dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:visibility="gone"
                        android:orientation="horizontal">

                        <androidx.cardview.widget.CardView
                            android:layout_width="200dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="50dp"
                            android:elevation="55dp"
                            app:cardCornerRadius="5dp"
                            app:cardPreventCornerOverlap="true"
                            app:cardUseCompatPadding="true">

                            <Button
                                android:id="@+id/btnpassword"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/button_shape"
                                android:padding="10dp"

                                android:text="Change Password"
                                android:textColor="@color/colorBlack"
                                android:textStyle="bold" />

                        </androidx.cardview.widget.CardView>
                    </LinearLayout>

                    <TextView
                        android:id="@+id/tvSignout"
                        android:layout_width="63dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="end"
                        android:layout_marginTop="10dp"
                        android:layout_marginRight="25dp"
                        android:layout_marginBottom="20dp"
                        android:onClick="Signout"
                        android:text="Sign out"
                        android:textColor="@color/colorWhite"
                        android:textStyle="bold"
                        tools:ignore="OnClick">

                    </TextView>

                </LinearLayout>
            </ScrollView>
        </androidx.cardview.widget.CardView>
    </RelativeLayout>



